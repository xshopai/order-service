{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "dapr-start",
      "postDebugTask": "dapr-stop",
      "program": "${workspaceFolder}/OrderService.Api/bin/Debug/net8.0/OrderService.Api.dll",
      "args": [],
      "cwd": "${workspaceFolder}/OrderService.Api",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "http://localhost:1006",
        "DAPR_HTTP_PORT": "3506",
        "DAPR_GRPC_PORT": "50006"
      },
      "console": "integratedTerminal",
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
        "uriFormat": "%s/swagger"
      }
    }
  ]
}
